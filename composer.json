{
    "name": "ahmard/flutterwave",
    "description": "Flutterwave PHP client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmard",
            "email": "Ahmard06@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ahmard/guzwrap": "^2.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.88",
        "symfony/var-dumper": "^5.2",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Ahmard\\Flutterwave\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ahmard\\Flutterwave\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan analyse"
    }
}
